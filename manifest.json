{
  "manifest_version": 2,

  "name": "fade-time",
  "description": "Fade the browser after a given amount of time",
  "version": "1.0",

  "browser_action": {
    "default_icon": "fade_icon.png",
    "default_popup": "popup.html",
    "default_title": "Configure"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
